{
  "name": "@cloakcraft/sdk",
  "version": "0.1.0",
  "description": "CloakCraft SDK for interacting with the privacy protocol",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./idl": {
      "types": "./dist/idl/index.d.ts",
      "import": "./dist/idl/index.mjs",
      "require": "./dist/idl/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/idl/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@cloakcraft/types": "workspace:*",
    "@coral-xyz/anchor": "^0.32.1",
    "@lightprotocol/stateless.js": "0.22.1-alpha.7",
    "@noble/ciphers": "^2.1.1",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@solana/spl-token": "^0.4.0",
    "bn.js": "^5.2.2",
    "bs58": "^6.0.0",
    "circomlibjs": "^0.1.7",
    "snarkjs": "^0.7.5"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.91.0",
    "@types/bn.js": "^5.2.0",
    "@types/bs58": "^5.0.0",
    "@types/snarkjs": "^0.7.9",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.91.0"
  }
}
