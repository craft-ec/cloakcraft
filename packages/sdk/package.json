{
  "name": "@cloakcraft/sdk",
  "version": "0.1.0",
  "description": "CloakCraft SDK for interacting with the privacy protocol",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@cloakcraft/types": "workspace:*",
    "@coral-xyz/anchor": "^0.32.1",
    "@lightprotocol/stateless.js": "0.22.1-alpha.7",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@noir-lang/noir_js": "^1.0.0-beta.18",
    "@noir-lang/noirc_abi": "^1.0.0-beta.18",
    "@solana/spl-token": "^0.4.0",
    "circomlibjs": "^0.1.7",
    "snarkjs": "^0.7.4"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.91.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.91.0"
  }
}
