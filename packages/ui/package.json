{
  "name": "@cloakcraft/ui",
  "version": "0.1.0",
  "description": "React UI components for CloakCraft privacy protocol",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external @cloakcraft/sdk --external @cloakcraft/hooks --external @solana/wallet-adapter-react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external @cloakcraft/sdk --external @cloakcraft/hooks --external @solana/wallet-adapter-react --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cloakcraft/hooks": "workspace:*",
    "@cloakcraft/sdk": "workspace:*",
    "@cloakcraft/types": "workspace:*"
  },
  "devDependencies": {
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.91.0",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/web3.js": "^1.91.0",
    "react": "^18.2.0"
  }
}
