# CloakCraft Web Configuration

# Solana RPC URL
NEXT_PUBLIC_RPC_URL=https://api.devnet.solana.com

# WebSocket URL for transaction confirmations (derive from RPC URL)
# For Helius: wss://devnet.helius-rpc.com/?api-key=YOUR_API_KEY
NEXT_PUBLIC_WSS_URL=

# Indexer URL for merkle proofs
NEXT_PUBLIC_INDEXER_URL=https://indexer.cloakcraft.io

# CloakCraft Program ID
NEXT_PUBLIC_PROGRAM_ID=FKaC6fnSJYBrssPCtwh94hwg3C38xKzUDAxaK8mfjX3a

# Network (devnet or mainnet-beta)
NEXT_PUBLIC_NETWORK=devnet

# Helius API Key (optional, for Light Protocol)
NEXT_PUBLIC_HELIUS_API_KEY=

# Filebase Configuration (for IPFS ballot metadata storage)
# Get these from https://console.filebase.com/keys
FILEBASE_KEY=your_filebase_access_key
FILEBASE_SECRET=your_filebase_secret_key
FILEBASE_BUCKET=your_bucket_name
